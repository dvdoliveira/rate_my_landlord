<div class="title">
  <h1><span class="glyphicon glyphicon-search"></span>  Search</h1> 
</div>

<%if @search_results.empty?%> <p>No results found!</p> <%end%>	

<%@search_results.each do |landlord|%>
	<div class="row">
		<div class="col-md-6">
			<h3>
				<a href = "/landlords/<%=landlord.id%>"><%="#{landlord.full_name}"%></a>
			</h3>
		</div>
	</div>
<%end%>
